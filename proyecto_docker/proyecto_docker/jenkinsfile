pipeline {
    agent any

    stages {
        stage('docker build') {
            steps {
                bat "docker build -t caosbinario/homer_page:1.0.0-${BUILD_ID} ."
            }
        }

        stage('docker push') {
            steps {
                bat "docker push caosbinario/homer_page:1.0.0-${BUILD_ID}"
            }
        }
    }
}